{
  "dashboard": {
    "id": null,
    "uid": "mac-studio-cluster",
    "title": "Mac Studio Cluster Dashboard",
    "tags": ["cluster", "mac-studio", "ray"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "system_cpu_percent",
            "legendFormat": "CPU %",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 2,
        "title": "Memory Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "system_memory_percent",
            "legendFormat": "Memory %",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "id": 3,
        "title": "Ray Cluster Nodes",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "ray_nodes_count",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto"
        }
      },
      {
        "id": 4,
        "title": "Ray CPUs Available",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "ray_cpus_available",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ]
      },
      {
        "id": 5,
        "title": "Ray GPUs Available",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "ray_gpus_available",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "Service Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "face_detection_status",
            "format": "table",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          },
          {
            "expr": "embedding_status",
            "format": "table",
            "refId": "B",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          },
          {
            "expr": "llm_status",
            "format": "table",
            "refId": "C",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          },
          {
            "expr": "admin_status",
            "format": "table",
            "refId": "D",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ]
      },
      {
        "id": 7,
        "title": "Disk Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "system_disk_percent",
            "legendFormat": "Disk %",
            "refId": "A",
            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  },
  "overwrite": false,
  "folderId": null,
  "folderUid": ""
}


